{
    "extends": ["standard", "plugin:vue/essential"],
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "plugins": [
        "vue",
        "html",
        "promise"
    ],
    "rules": {
        "indent": "off",
        "camelcase": "off"
    }
}